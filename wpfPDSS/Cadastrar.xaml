                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.598"/>
                    <TranslateTransform X="-0.245" Y="0.636"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label Content="e-mail" HorizontalAlignment="Left" Margin="31,104,0,0" VerticalAlignment="Top" FontSize="22"/>
        <Label Content="sexo" HorizontalAlignment="Left" Margin="31,236,0,0" VerticalAlignment="Top" FontSize="22"/>
        <Label Content="data de nascimento" HorizontalAlignment="Left" Margin="31,167,0,0" VerticalAlignment="Top" Cursor="" FontSize="22"/>
        <Label Content="telefone" HorizontalAlignment="Left" Margin="31,311,0,0" VerticalAlignment="Top" FontSize="22"/>
        <TextBox HorizontalAlignment="Left" Margin="132,316,0,0" TextWrapping="Wrap" Text="&#xA;" VerticalAlignment="Top" Width="203" Height="34"/>
        <TextBox HorizontalAlignment="Left" Margin="112,247,0,0" TextWrapping="Wrap" Text="&#xA;" VerticalAlignment="Top" Width="198" TextChanged="TextBox_TextChanged_1" Height="28"/>
        <TextBox HorizontalAlignment="Left" Margin="237,167,0,0" TextWrapping="Wrap" Text="&#xA;" VerticalAlignment="Top" Width="259" Height="39"/>
        <TextBox HorizontalAlignment="Left" Margin="112,115,0,0" TextWrapping="Wrap" Text="&#xA;" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Margin="112,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186" RenderTransformOrigin="-0.393,1.203" TextChanged="TextBox_TextChanged"/>
        <Label Content="CADASTRAR" HorizontalAlignment="Left" Margin="261,14,0,0" VerticalAlignment="Top" Cursor="" FontSize="22"/>

    </Grid>
</Window>
