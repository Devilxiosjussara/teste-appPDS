<Window x:Class="WpfPDSS.telas.Cadastrar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfPDSS.telas"
        mc:Ignorable="d"
        Title="Cadastrar" Height="450" Width="800">
    <Grid>
        <Label Content="" HorizontalAlignment="Left" Margin="85,116,0,0" VerticalAlignment="Top" Height="31" Width="55"/>
        <Button Content="SALVAR" HorizontalAlignment="Left" Margin="540,316,0,0" VerticalAlignment="Top" Height="68" Width="145" Click="Button_Click" FontSize="20" FontWeight="Bold"/>
        <Label Content="," HorizontalAlignment="Left" Margin="49,55,0,0" VerticalAlignment="Top" Height="31" Width="43" RenderTransformOrigin="0.5,0.5" FontSize="22">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.598"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label Content="nome" HorizontalAlignment="Left" Margin="28,47,0,0" VerticalAlignment="Top" Height="61" Width="151" RenderTransformOrigin="1.926,1.243" FontSize="22">
